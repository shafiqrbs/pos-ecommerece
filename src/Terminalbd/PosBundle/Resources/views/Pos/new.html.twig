{% extends '::pos-base.html.twig' %}
{% block body %}
    {#{% if config.templateCss is not empty %}
    <style>{{ config.templateCss|raw }}</style>
    {% endif %}#}
    <div class="row-fluid" id="pos-area">
        <div class="portlet box">
            <div class="portlet-body">
                {% if invoiceMode == 'search' %}
                <div style="background: #e5e5e5; padding: 15px">
                    <div class="row-fluid">
                        <div class="span12">
                            <div class="input-prepend input-append span3" style="width: 250px!important;">
                                <span class="add-on add-on-height"><i class="icon-barcode"></i></span>
                                <input type="text" id="barcode" name="barcode" required="required" class="m-wrap span12" placeholder="0008000045" autocomplete="off" value="">
                            </div>
                            <form name="stockItem" id="stockItem" method="post" action="{{ path('pos_item_create') }}">
                                <div class="input-prepend input-append span9">
                                    {% if globalOption.inventoryConfig.salesMode == "purchase-item" %}
                                    <input type="text" id="item" name="item" class="span5 m-wrap m-wrap-bg stockInput select2StockItem" value="" autocomplete="off"  placeholder="Search product">
                                    <select class="span3 m-wrap m-wrap-bg" style="height: 50px!important;" id="purchaseItem" name="purchaseItem">
                                        <option>Select Barcode</option>
                                    </select>
                                    <select class="span2 m-wrap m-wrap-bg" style="height: 50px!important;" id="serialNo" name="serialNo">
                                        <option>Select Serial No</option>
                                    </select>
                                    {% else %}
                                    <input type="text" id="item" name="item" class="span7 m-wrap m-wrap-bg stockInput select2StockItem" value="" autocomplete="off"  placeholder="Search product">
                                    {% endif %}
                                    <input type="text" id="quantity" name="quantity" class="span m-wrap m-wrap-bg pull-center stockInput amount" style="width: 80px;" value="" autocomplete="on"  placeholder="QTY" >
                                    <input type="text" id="price" name="price" class="span2 m-wrap m-wrap-bg stockInput pull-center amount" value="" autocomplete="on"  placeholder="Price" >
                                    <button type="submit" style="height: 50px" class="btn yellow big" id="addItem" name="addItem" value="addItem"><i class="icon-print"></i> Add Item</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                {% endif %}

                <div class="row-fluid">
                    {% if invoiceMode == 'search' %}
                        <div class="span8">
                            <div class="row-fluid">
                                <div class="span12">
                                    <table class="table table-bordered">
                                        <thead>
                                        <tr class="head-gray">
                                            <th class="numeric product-list">Current Stock</th>
                                            <th class="numeric product-list" id="current-stock"></th>
                                            <th class="numeric product-list">Sales Price</th>
                                            <th class="numeric product-list" id="sales-price"></th>
                                            <th class="numeric product-list">Status</th>
                                            <th class="numeric product-list" id="item-status"></th>
                                        </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <table class="table table-bordered">
                                <thead class="flip-content ">
                                <tr class="head-gray">
                                    <th class="numeric">&nbsp;</th>
                                    <th class="numeric span8 product-title-name">Product</th>
                                    <th class="numeric product-price pull-center">Price</th>
                                    <th class="numeric span2 pull-center">Quantity</th>
                                    <th class="numeric product-price">Total</th>
                                    <th class="numeric"></th>
                                </tr>
                                </thead>
                                <tbody id="invoiceItems">
                                {% if  cart.contents is not empty %}
                                    {% for item in cart.contents %}
                                        <tr id="remove-{{ item.rowid }}">
                                            <th>{{ loop.index }}.</th>
                                            <td class="numeric product-title">
                                                {{ item.name }}
                                                {% if item.item.serial is defined and item.item.serial is not empty %} <br/>IME/Serial no : {{ item.serial }}{% endif %}
                                            </td>
                                            <td class="numeric product-price">
                                                {{ item.price }}
                                                <input type="hidden" class="form-control inline-m-wrap updateProductPrice btn-qnt-particular" data-id="{{ item.rowid }}"  id="price-{{ item.rowid }}" name="price-{{ item.rowid }}"  value="{{ item.price }}" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" >
                                            </td>
                                            <td class="product-quantity">
                                                <div class="input-append" style="margin-bottom: 0!important;">
                                                    <span class="input-group-btn">
  <a href="javascript:" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" class="btn green btn-number-pos mini" data-type="minus" data-id="{{ item.rowid }}"  data-text="{{ item.rowid }}" data-title="{{ item.price }}"  data-field="quantity"> <span class="fa fa-minus"></span> </a>
                                                       <input type="text" class="form-control inline-m-wrap updateProduct btn-qnt-particular" data-id="{{ item.rowid }}"  id="quantity-{{ item.rowid }}" name="quantity-{{ item.rowid }}" data-title="{{ item.price }}" value="{{ item.quantity }}" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" min="1" max="{{ item.maxQuantity }}">
                                                      <a href="javascript:" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" class="btn green btn-number-pos mini"  data-type="plus" data-id="{{ item.rowid }}" data-title="{{ item.price }}"  data-text="{{ item.rowid }}" data-field="quantity"> <span class="fa fa-plus"></span></a></span>
                                                </div>
                                            </td>
                                            <td class="numeric product-price"><strong id="subTotal-{{ item.rowid }}">{{ (item.quantity * item.price)|number_format(2) }}</strong></td>
                                            <td class="numeric">
                                                <a id="{{ item.rowid }}" data-id="{{ item.rowid }}" data-url="{{ path('pos_item_remove',{'product':item.rowid}) }}" href="javascript:" class="btn red mini particularDelete"><i class="icon-trash"></i></a>
                                            </td>
                                        </tr>
                                    {% endfor %}
                                {% endif %}
                                </tbody>
                                <tfoot>
                                <tr><td colspan="6" style="border-bottom: 2px solid #3598dc">&nbsp;</td></tr>
                                <tr class="head-gray">
                                    <th colspan="4" class="product-title text-right">Sub-Total</th>
                                    <th id="" class="product-title"><span class="subTotal">{{ cart.total|number_format(2) }}</span></th>
                                    <td>&nbsp;</td>
                                </tr>
                                </tfoot>
                            </table>
                            <div class="clearfix"></div>
                        </div>
                    {% else %}
                        <div class="span8">
                            <div class="row-fluid">
                                <div class="tabbable tabbable-custom" style="float: left; width: 100%">
                                    <div class="tab-content">
                                        <div class="tab-pane tab-height active" id="all-product">
                                            <div class="row-fluid productBox" >
                                                <div class="loader loader-double"></div>
                                                <div class="" id="items" style="margin-top:56px!important;"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    {% endif %}
                    <div class="span4">
                        <div class="row-fluid">
                            <div class="portlet box royal-blue">
                                <div class="portlet-body">
                                    {{ form_start(form) }}
                                    {{ form_row(form._token) }}
                                    <div class="transactionBox" id="transaction-box">
                                        <div class="payment-info">
                                            {% if invoiceMode != 'search' %}
                                                <div class="clearfix"></div>
                                                <table class="table table-bordered">
                                                    <thead class="flip-content ">
                                                    <tr class="head-royal-blue">
                                                        <th class="numeric">&nbsp;</th>
                                                        <th class="numeric span8 product-title-name">Product</th>
                                                        <th class="numeric product-price pull-center">Price</th>
                                                        <td class="numeric">&nbsp;</td>
                                                        <th class="numeric product-price">Total</th>
                                                        <th class="numeric"></th>
                                                    </tr>
                                                    </thead>
                                                    <tbody id="invoiceItems">
                                                    {% set totalDiscount = 0 %}
                                                    {% if  cart.contents is not empty %}
                                                        {% for item in cart.contents %}
                                                            {% set totalDiscount = totalDiscount + (item.quantity * item.discountPrice)  %}
                                                            <tr id="remove-{{ item.rowid }}">
                                                                <td>{{ loop.index }}.</td>
                                                                <td class="numeric product-title">{{ item.name }}</td>
                                                                <td class="numeric product-price">
                                                                    {{ item.price }}
                                                                    <input type="hidden" class="form-control inline-m-wrap updateProductPrice btn-qnt-particular" data-id="{{ item.rowid }}"  id="price-{{ item.rowid }}" name="price-{{ item.rowid }}"  value="{{ item.price }}" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}"  >
                                                                </td>
                                                                <td class="product-quantity text-center">
                                                                    <div class="input-append" style="margin-bottom: 0!important;">
                                                                <span class="input-group-btn">
              <a href="javascript:" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" class="btn green btn-number-pos mini" data-type="minus" data-id="{{ item.rowid }}"  data-text="{{ item.rowid }}"  data-field="quantity"> <span class="fa fa-minus"></span> </a>
                                                                   <input type="text" class="form-control inline-m-wrap updateProduct btn-qnt-particular" data-id="{{ item.rowid }}"  id="quantity-{{ item.rowid }}" name="quantity-{{ item.rowid }}"  value="{{ item.quantity }}" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" min="1" max="{{ item.maxQuantity }}">
                                                                  <a href="javascript:" data-action="{{ path('pos_item_update',{'product':item.rowid}) }}" class="btn green btn-number-pos mini"  data-type="plus" data-id="{{ item.rowid }}" data-title="{{ item.price }}"  data-text="{{ item.rowid }}" data-field="quantity"> <span class="fa fa-plus"></span></a></span>
                                                                    </div>
                                                                </td>
                                                                <td class="numeric product-price"><strong id="subTotal-{{ item.rowid }}">{{ (item.quantity * item.price)|number_format(2) }}</strong></td>
                                                                <td class="numeric">
                                                                    <a id="{{ item.rowid }}" data-id="{{ item.rowid }}" data-url="{{ path('pos_item_remove',{'product':item.rowid}) }}" href="javascript:" class="btn red mini particularDelete"><i class="icon-trash"></i></a>
                                                                </td>
                                                            </tr>
                                                        {% endfor %}
                                                    {% endif %}
                                                    </tbody>
                                                    <tfoot>
                                                    <tr><td colspan="6" style="border-bottom: 2px solid #3598dc">&nbsp;</td></tr>
                                                    <tr>
                                                        <th colspan="4" class="product-title text-right">Sub-Total</th>
                                                        <th class="product-title"><span class="subTotal">{{ entity.subTotal|number_format(2) }}</span></th>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <th colspan="4" class="product-title text-right">Discount</th>
                                                        <th class="product-title"><span class="discount">{{ entity.discount|number_format(2) }}</span></th>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <th colspan="4" class="product-title text-right">Special Discount</th>
                                                        <th class="product-title"><span class="specialDiscount">{{ entity.specialDiscount|number_format(2) }}</span></th>
                                                        <td></td>
                                                    </tr>special
                                                    <tr><td colspan="6" style="border-bottom: 1px solid #3598dc">&nbsp;</td></tr>
                                                    <tr>
                                                        <th colspan="4" class="product-title text-right">Total</th>
                                                        <th class="product-title"><span class="total">{{ entity.total|number_format(2) }}</span></th>
                                                        <td></td>
                                                    </tr>
                                                    </tfoot>
                                                </table>
                                            {% endif %}
                                            <div class="row-fluid">
                                                <div class="span12">{{ form_widget(form.salesBy) }}</div>
                                            </div>
                                            <div class="clearfix"></div>
                                            <div class="row-fluid" style="margin: 2px 0 2px 0;">
                                                <div class="span10">
                                                    <div class="input-prepend input-append span12" >
                                                        <input type="text" style="margin-left:0!important;" id="mobile" name="mobile"  class="span12 m-wrap select2Customer" value=""  autocomplete="off" placeholder="Customer mobile no">
                                                    </div>
                                                </div>
                                                <div class="span2">
                                                    <span class="btn-lg span12 addCustomer btn" style="border: 4px solid #d93616"><i class="icon-user"></i></span>
                                                </div>
                                            </div>
                                            <div class="clearfixboth"></div>
                                            <div class="payment-box" style="padding: 0!important;padding-left: 7px!important;">
                                                <div class="row-fluid">
                                                    <div class="span3" style="font-weight: bold; font-size: 12px;padding-top: 6px">Special Discount</div>
                                                    <div class="span3">
                                                        {{ form_widget(form.specialDiscount) }}
                                                    </div>
                                                    <div class="span3" style="font-weight: bold; font-size: 12px;padding-top: 6px">Current Credit: </div>
                                                    <input type="hidden" name="outstandingHidden" id="outstandingHidden">
                                                    <div class="span3"><strong id="outstanding" style="color: red; font-weight: bold; font-size: 16px; padding-top: 6px; display: inline-block"></strong></div>
                                                </div>
                                            </div>
                                            <div class="payment-box customer">
                                                <div class="row-fluid customer" id="">
                                                    <div class="span8">
                                                        <div class="row-fluid">
                                                            <div class="input-icon input-height">
                                                                <input type="text" id="customerName" name="customerName" class="m-wrap span12 customer-input" autocomplete="off" placeholder="Customer name" value="">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="span4">
                                                        <div class="row-fluid">
                                                            <div class="input-icon input-height">
                                                                <input type="text" id="employeeId" name="employeeId" class="m-wrap span12 customer-input" autocomplete="off" placeholder="Employee ID" value="">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfixboth"></div>
                                                <div class="row-fluid">
                                                    <div class="span8">
                                                        <input type="text" id="customerMobile" name="customerMobile" class="m-wrap span12 mobile customer-input" autocomplete="off" placeholder="Customer mobile no" value="">
                                                    </div>
                                                    <div class="span4">
                                                        <div class="input-icon input-height">
                                                            <select class="m-wrap span12 customer-input" name="gender" id="gender">
                                                                <option value="F">Female</option>
                                                                <option value="M">Male</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfixboth"></div>
                                                <div class="row-fluid">
                                                    <div class="span6">
                                                        <select class="m-wrap span12 section" data-id="profession" id="profession" name="profession" >
                                                            <option value="">Select Profession</option>
                                                            {% if professions is not empty %}
                                                                {% for key,profession in professions  %}
                                                                    <option  value="{{ profession.particular }}">{{ profession.particular }}</option>
                                                                {% endfor %}
                                                            {% endif %}
                                                        </select>
                                                    </div>
                                                    <div class="span6">
                                                        <select class="m-wrap span12 section" data-id="section" id="section" name="section" >
                                                            <option value="">Select Section</option>
                                                            {% if sections is not empty %}
                                                                {% for key,section in sections  %}
                                                                    <option  value="{{ section.particular }}">{{ section.particular }}</option>
                                                                {% endfor %}
                                                            {% endif %}
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="clearfixboth"></div>
                                                <div class="row-fluid">
                                                    <div class="span12">
                                                        <select class="m-wrap span12 unit" data-id="unit" id="unit" name="unit" >
                                                            <option value="">Select Unit</option>
                                                            {% if units is not empty %}
                                                                {% for key,unit in units  %}
                                                                    <option  value="{{ unit.particular }}">{{ unit.particular }}</option>
                                                                {% endfor %}
                                                            {% endif %}
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="clearfixboth"></div>
                                                <div class="row-fluid">
                                                    <div class="span6">
                                                        <select class="m-wrap span12 section" data-id="department" id="department" name="department">
                                                            <option value="">Select Department</option>
                                                            {% if sections is not empty %}
                                                                {% for key,department in departments  %}
                                                                    <option  value="{{ department.particular }}">{{ department.particular }}</option>
                                                                {% endfor %}
                                                            {% endif %}
                                                        </select>
                                                    </div>
                                                    <div class="span6">
                                                        <select class="m-wrap span12 customerCategory" data-id="customerCategory" id="customerCategory" name="customerCategory">
                                                            <option value="">Select Category</option>
                                                            {% if customerCategories is not empty %}
                                                                {% for key,category in customerCategories  %}
                                                                    <option  value="{{ category.particular }}">{{ category.particular }}</option>
                                                                {% endfor %}
                                                            {% endif %}
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="clearfix"></div>
                                            <div class="row-fluid">
                                                <div class="span12">
                                                    <div class="switch-method">
                                                        {% set methods = ['Credit','Cash','Mobile','Bank'] %}
                                                        {% for method in methods %}
                                                            <input {% if entity.transactionMethod is not empty and entity.transactionMethod.name == method %} checked="checked" {% endif %} type="radio" id="{{ method }}" class="method-process" name="method" value="{{ method }}"/>
                                                            <label  for="{{ method }}">{{ method }}</label>
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="clearfix"></div>
                                            <div id="cartMethod" class="bankHide">
                                                <div class="payment-box">
                                                    <div class="row-fluid">
                                                        <div class="span12">{{ form_widget(form.accountBank) }}</div>
                                                    </div>
                                                    <div class="clearfixboth"></div>
                                                    <div class="row-fluid">
                                                        <div class="span6">{{ form_widget(form.paymentCard) }}</div>
                                                        <div class="span6">{{ form_widget(form.cardNo) }}</div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div id="bkashMethod" class="mobileBankHide">
                                                <div class="payment-box">
                                                    <div class="row-fluid">
                                                        <div class="span12">{{ form_widget(form.accountMobileBank) }}</div>
                                                    </div>
                                                    <div class="clearfixboth"></div>
                                                    <div class="row-fluid">
                                                        <div class="span6">{{ form_widget(form.paymentMobile) }}</div>
                                                        <div class="span6">{{ form_widget(form.transactionId) }}</div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="row-fluid text-center" style="background: #3598dc; padding-top: 15px; padding-bottom: 15px">
                                        <div class="input-prepend input-append span12" >
                                            {% if entity.total > entity.payment %}
                                            <span class="add-on add-on-height yellow"><strong id="balance">Due {{ (entity.total - entity.payment)|number_format(2) }}</strong></span>
                                            {% else %}
                                            <span class="add-on add-on-height yellow"><strong id="balance">Return {{ (entity.total - entity.payment)|abs|number_format(2) }}</strong></span>
                                            {% endif %}
                                            {{ form_widget(form.payment) }}
                                            {{ form_widget(form.discountType) }}
                                            <span class="add-on add-on-height" id="btn-refresh"><strong>C</strong></span>
                                            <span class="add-on add-on-height" >{{ form_widget(form.isHold) }}</span>
                                        </div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="pull-center">
                                            <input type="hidden" class="" name="invoiceEntity" id="invoiceEntity" value="{{ entity.id }}" >
                                            <input type="hidden" class="" name="invoiceEntity" id="invoiceEntity" value="{{ entity.id }}" >
                                            <input type="hidden" class="total due" name="total" id="total" value="{{ entity.total }}" >
                                            <input type="hidden" class="due" name="due" id="due" value="{{ entity.total }}" >
                                            <input type="hidden" name="discount" id="discount" value="">
                                            <input type="hidden" name="buttonType" id="buttonType" value="posBtn">

                                        </div>
                                    </div>
                                    {{ form_end(form) }}
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div style="height: 60px; position: fixed; bottom:0; background: #dddddd; width: 100%; padding-top: 7px">
       <div class="container-fluid">
           <div class="row-fluid" id="process-box">
               <div class="span4">
                   <div class="switch-field">
                       {#<input type="radio" data-id="" id="search" {% if invoiceMode == 'search' %} checked="checked" {% endif %} data-action="{{ path('pos_invoice_mode',{'mode':'search'}) }}" class="invoice-mode" name="process" value="search">
                       <label for="search"><i class="icon-search"></i> Search</label>
                       <input type="radio" data-id="" id="list" {% if invoiceMode == 'list' %} checked="checked" {% endif %}  data-action="{{ path('pos_invoice_mode',{'mode':'list'}) }}" class="invoice-mode" name="process" value="list">
                       <label for="list"><i class="icon-th-list"></i> List</label>
                       <input type="radio" data-id="" id="grid" {% if invoiceMode == 'grid' %} checked="checked" {% endif %}  data-action="{{ path('pos_invoice_mode',{'mode':'grid'}) }}" class="invoice-mode" name="process" value="grid">
                       <label for="grid"><i class="icon-th-large"></i> Grid</label>#}
                   </div>
               </div>
               <div class="span4">
                   <div class="pull-right">
                       <div class="btn-group">
                           {% if invoiceMode != 'search' %} %}
                           <div id="menu">
                               <ul>
                                   <li><center><a href="#all-product" data-toggle="tab"  id="allproduct" aria-expanded="false">All Products</a></center>
                                       <ul>
                                           {% for category in categories %}
                                               <li><a href="#{{ category.slug }}" data-action="{{ path('pos_category_stock_item',{'id':category.category_id}) }}" data-toggle="tab" class="saveButton category" aria-expanded="false">{{ category.name }}</a></li>
                                           {% endfor %}
                                       </ul>
                                   </li>
                               </ul>
                           </div>
                           <a href="javascript:" id="item-reload"  class="btn red big"><i class="icon-refresh"></i> Stock Reload</a>
                           {% endif %}
                           {#<a href="javascript:" id="item-reload"  class="btn green big"><i class="icon-eye-open"></i> Preview</a>#}
                       </div>
                   </div>
               </div>
               <div class="span4">
                   <div class="switch-progress">
                       <input type="radio" data-id="pos" id="POS" data-action="{{ path('pos_create',{'mode':'pos'}) }}" class="invoice-process" name="progress" value="pos">
                       <label for="POS" class="pos"><i class="icon-print"></i> POS</label>
                       <input type="radio" data-id="save" id="Save" data-action="{{ path('pos_create',{'mode':'save'}) }}" class="invoice-process" name="progress" value="save">
                       <label for="Save" class="save"><i class="icon-save"></i> Save</label>
                       <input type="radio" data-id="save-hold" id="save-hold" data-action="{{ path('pos_create',{'mode':'save-hold'}) }}" class="invoice-process" name="progress" value="save-hold">
                       <label for="save-hold" class="save-hold"><i class="icon-pencil"></i> Hold</label>
                   </div>
               </div>

           </div>
       </div>
    </div>
<style>

    #menu * {
        padding:0;
        margin: 0;
        list-style-type:none;
        font-size: 14px;
    }
    #menu {
        float: left;
        line-height:30px;
      }
    #menu a {
        display: block;
        text-decoration: none;
        color: #ffffff;
    }
    #menu a:hover { background: #158bdc;}
    #menu ul li ul li a:hover {
        background: #158bdc;
        color:#ffffff;
    }
    #menu ul li ul li {
        width: 244px;
        border: none;
        color: #000000;
        padding: 3px;
        background:#dddddd;
        z-index: 1;
        height: 36px;
        border-top: 1px solid #b1b2b3;
        line-height:30px;
    }
    #menu ul li ul li a {
        padding-top:3px;
        padding-bottom:3px;
        color: #000000;
    }
    #menu ul li {
        float: left;
        width: 250px;
