    $(document).on("change", ".select2Customer", function() {
        var customer = $(this).val();
        $.get( Routing.generate('domain_customer_sales_ledger'),{ customer:customer} )
        .done(function( data ) {
            obj = JSON.parse(data);
            $('#outstanding').html(obj['creditText']);
            $('#outstandingHidden').val(obj['credit']);
        });
    });
